{
  "expo": {
    "name": "AniUp",
    "slug": "aniup",
    "version": "1.0.0",
    "scheme": "aniup",
    "orientation": "portrait",
    "icon": "./assets/aniyp.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/aniyp.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.up.aniup"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/aniyp.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.up.aniup"
    },
    "web": {
      "favicon": "./assets/favicon.png"
    }
  }
}
