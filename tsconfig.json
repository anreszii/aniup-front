{
  "extends": "expo/tsconfig.base",
  "compilerOptions": {
    "strict": true,
    "baseUrl": "./",
    "paths": {
      "assets/*": ["./assets/*"],
      "lib/*": ["src/lib/*"],
      "ui/*": ["./src/ui/*"],
      "entities/*": ["./src/entities/*"],
      "components/*": ["./src/components/*"],
      "modules/*": ["./src/modules/*"],
      "pages/*": ["./src/pages/*"]
    }
  }
}
